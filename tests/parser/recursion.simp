
/*
 * Syntax test for recursive functions.
 */

namespace recursion {

    int recursive_func(int val) {
        val = val + 1
        if(val > 10) {
            return val
        }
        else {
            return recursive_func(val)
        }
    }

}

entry {

    recursive_func(2)
    recursive_func(3)
    recursive_func(4)

}
