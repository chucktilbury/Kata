;-------------------------------------------------------------------------------
; Basic permutations of while
;-------------------------------------------------------------------------------

func name()() {

    while {
        name1 = 123
    }

    while() {
        name2 = 234
    }

    while(1) {
        name3 = 345
    }

    while("a string") {
        name4 = 456
    }

    while((1+2)!=4) {
        name5 = 567
    }
}
